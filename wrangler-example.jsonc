{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "femboymap",
  "pages_build_output_dir": "./public",
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "<YOUR_D1_DATABASE_NAME>",
      "database_id": "<YOUR_D1_DATABASE_ID>"
    }
  ],
  "env": {
    "preview": {
      "d1_databases": [
        {
          "binding": "DB",
          "database_name": "<YOUR_D1_DATABASE_NAME>",
          "database_id": "<YOUR_D1_DATABASE_ID>"
        }
      ]
    }
  }
}