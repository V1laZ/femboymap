{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "femboymap",
  "main": "worker/index.js",
  "compatibility_date": "2025-08-03",
  "compatibility_flags": [
    "global_fetch_strictly_public"
  ],
  "assets": {
    "directory": "./public"
  },
  "observability": {
    "enabled": true
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "<YOUR_D1_DATABASE_NAME>",
      "database_id": "<YOUR_D1_DATABASE_ID>"
    }
  ],
}